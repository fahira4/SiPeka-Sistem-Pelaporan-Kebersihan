<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="test.controller.HalamanLogin">
    <children>
        <VBox alignment="CENTER" spacing="10" AnchorPane.topAnchor="50.0" AnchorPane.leftAnchor="50.0">
            <Label text="Selamat Datang di SiPeka!!!"/>
            <TextField fx:id="usernameField" promptText="Username"/>
            <PasswordField fx:id="passwordField" promptText="Password"/>
            <Label fx:id="errorLabel" textFill="red"/>
            <Button text="Login" onAction="#handleLogin"/>
        </VBox>
    </children>
</AnchorPane>
